{
    "name": "fn-api-services",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "start:dev": "nest start --watch",
        "build": "tsc -p tsconfig.build.json",
        "prepare": "npm run build",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:prod": "node dist/main",
        "test": "NODE_ENV=development jest",
        "test:watch": "jest --verbose --watchAll",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "typeorm-ts-node-commonjs --config src/config/orm.config.ts",
        "m:g": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -d src/config/orm.config.ts",
        "m:r": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/config/orm.config.ts",
        "m:c": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create -d src/config/orm.config.ts",
        "m:rev": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/config/orm.config.ts",
        "command": "npx nestjs-command",
        "console": "node dist/console.js",
        "sonar": "node sonar-project.js"
    },
    "dependencies": {
        "@nestjs/axios": "^2.0.0",
        "@nestjs/bull": "^0.6.0",
        "@nestjs/common": "^9.0.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/typeorm": "^9.0.1",
        "@servicelabsco/nestjs-utility-services": "^1.1.8",
        "@socket.io/redis-adapter": "^8.1.0",
        "bull": "^4.8.5",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "compression": "^1.7.4",
        "date-fns": "^2.29.2",
        "helmet": "^5.1.1",
        "nestjs-console": "^8.0.0",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "socket.io": "^4.6.1",
        "typeorm": "^0.3.7",
        "typeorm-naming-strategies": "^4.1.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/bull": "^3.15.9",
        "@types/express": "^4.17.13",
        "@types/ioredis": "^5.0.0",
        "@types/jest": "28.1.4",
        "@types/node": "^16.0.0",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "28.1.2",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "28.0.5",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
